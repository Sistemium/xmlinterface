<?xml version="1.0"?>
<view-definition xmlns="http://unact.net/xml/xi" name="wms-pregress-view" label="Прогресс инвентаризации">
    <view-schema>
        <form name="sysuser">
            <parameter name="name">
                <init with="username"/>
            </parameter>
 
            <form name="wms-progress" is-set="true" >
                <all-labeled-fields />                
            </form>
            
        </form>
    </view-schema>
    <workflow>
        <step>
            <display>
                <grid form="wms-progress"/>
            </display>
        </step>
    </workflow>
</view-definition>