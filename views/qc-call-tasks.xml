<?xml version="1.0"?>
<view-definition xmlns="http://unact.net/xml/xi" name="qc-call-tasks" label="Задания на обзвон">
    <view-schema>
        <form name="qc-operator">
            <parameter name="login">
                <init with="username"/>
            </parameter>
            <form name="qc-call-task" label="Задания на обзвон" is-set="true" page-size="10">
                <field name="id" />
                <field name="title" />
                <field name="cts" format="smalltime" />
                <order-by name="id" dir="desc"/>
            </form>
        </form>
    </view-schema>
    <workflow>
        <step>
            <display>
                <grid form="qc-call-task"/>
            </display>
        </step>
    </workflow>
</view-definition>